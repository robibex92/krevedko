{
  "name": "online-orders-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/server.v2.js",
  "scripts": {
    "dev": "nodemon --watch src --ext js --exec \"node src/server.v2.js\"",
    "dev:old": "nodemon --watch src --ext js --exec \"node src/server.js\"",
    "build": "echo \"Backend is Node/Express - no build step\"",
    "start": "node src/server.v2.js",
    "start:old": "node src/server.js",
    "lint": "eslint . --ext js --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js --fix",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node src/seed.js"
  },
  "prisma": {
    "seed": "node src/seed.js",
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "heic-convert": "^2.1.0",
    "helmet": "^7.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-mailru": "^0.0.1",
    "passport-yandex": "^0.0.5",
    "redis": "^5.8.3",
    "sharp": "^0.34.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.4.2",
    "prisma": "^5.22.0"
  }
}
